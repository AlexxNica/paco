<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:paco="urn:import:com.google.sampling.experiential.client">
  <ui:style>
  </ui:style>
  <g:HTMLPanel>
    <dl>
      <dt>Title</dt>
      <dd><g:TextBox ui:field="title" /></dd>

      <dt>Description</dt>
      <dd><g:TextArea ui:field="description" /></dd>

      <dt>Consent Form</dt>
      <dd><g:TextArea ui:field="consentForm" /></dd>

      <dt>Signal Schedule</dt>
      <dd><paco:SignalScheduleView ui:field="signalSchedule" /></dd>

      <dt>Inputs</dt>
      <g:FlowPanel ui:field="inputs" />

      <dt>Feedback</dt>
      <dd><g:TextArea ui:field="feedback" /></dd>

      <dt>Options</dt>
      <dd><g:CheckBox ui:field="published">Published</g:CheckBox></dd>
    </dl>
  </g:HTMLPanel>
</ui:UiBinder>
