<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:paco="urn:import:com.google.sampling.experiential.client">
  <ui:style>
  </ui:style>
  <g:HTMLPanel>
    <g:ListBox ui:field="types" />
    Name
    <g:TextBox ui:field="name" />
    <g:CheckBox ui:field="required">Required</g:CheckBox>
    Conditional Expression
    <g:TextBox ui:field="conditionalExpression" />
    <g:Button ui:field="add">+</g:Button>
    <g:Button ui:field="remove">-</g:Button>

    <paco:TextInputView ui:field="textInput" visible="false" />
    <paco:LikertInputView ui:field="likertInput" visible="false" />
    <paco:ListInputView ui:field="listInput" visible="false" />
  </g:HTMLPanel>
</ui:UiBinder>
